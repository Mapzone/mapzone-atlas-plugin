<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.rap.ui.entrypoint">
      <entrypoint
            brandingId="io.mapzone.atlas.branding"
            class="org.polymap.rhei.batik.BatikApplication"
            id="io.mapzone.atlas.entrypoint"
            path="/atlas">
      </entrypoint>
   </extension>
   <extension
         point="org.polymap.rhei.batik.design">
      <design
            class="io.mapzone.atlas.ui.AtlasAppDesign"
            id="io.mapzone.atlas.design"
            servletNameMatcher="/atlas">
      </design>
   </extension>
   <extension
         point="org.eclipse.rap.ui.branding">
      <branding
            id="io.mapzone.atlas.branding"
            themeId="org.polymap.rhei.batik.materialDesign"
            title="Atlas">
         <additionalHeaders>
            <link
                  href="batikres/css/md/flowtext.css"
                  rel="stylesheet">
            </link>
            <link
                  href="batikres/css/roboto.css"
                  rel="stylesheet">
            </link>
         </additionalHeaders>
      </branding>
   </extension>
   <extension
         point="org.polymap.rhei.batik.panels">
      <panel
            class="io.mapzone.atlas.ui.SearchPanel"
            id="io.mapzone.atlas.AtlasSearchPanel"
            stackPriority="100">
      </panel>
      <panel
            class="io.mapzone.atlas.ui.AtlasMapPanel"
            id="io.mapzone.atlas.AtlasMapPanel">
      </panel>
      <panel
            class="io.mapzone.atlas.ui.AtlasLayersPanel"
            id="io.mapzone.atlas.AtlasLayersPanel"
            stackPriority="10">
      </panel>
      <panel
            class="io.mapzone.atlas.sheet.LayerSheetsHelpPanel"
            id="io.mapzone.atlas.LayerSheetsHelpPanel">
      </panel>
      <panel
            class="io.mapzone.atlas.ui.LegalPanel"
            id="io.mapzone.atlas.LegalPanel">
      </panel>
      <panel
            class="io.mapzone.atlas.ui.DocPanel"
            id="io.mapzone.atlas.DocPanel">
      </panel>
   </extension>
   <extension
         point="org.polymap.rhei.batik.panelFilters">
      <panelFilter
            class="io.mapzone.atlas.ui.AtlasPanelFilter"
            servletNameMatcher="/atlas">
      </panelFilter>
      <panelFilter
            class="io.mapzone.atlas.ui.ExcludeAtlasPanelFilter"
            servletNameMatcher="/arena">
      </panelFilter>
      <panelFilter
            class="io.mapzone.atlas.ui.ExcludeAtlasPanelFilter"
            servletNameMatcher="/p4">
      </panelFilter>
   </extension>
   <extension
         point="org.polymap.core.data.pipeline.builderConcerns">
      <listener
            class="io.mapzone.atlas.AtlasQueryFilterProcessor$AtlasQueryFilterPipelineBuilderConcern"
            id="io.mapzone.atlas.FulltextFilterProcessor"
            name="FulltextFilterProcessor">
      </listener>
   </extension>
   <extension
         point="org.polymap.rhei.batik.contributions">
      <contributions
            class="io.mapzone.atlas.sheet.LayerSheetContribution"
            id="io.mapzone.atlas.LayerSheetContribution">
      </contributions>
   </extension>

</plugin>
