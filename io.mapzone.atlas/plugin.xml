<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.rap.ui.entrypoint">
      <entrypoint
            brandingId="org.polymap.rhei.batik.branding"
            class="org.polymap.rhei.batik.BatikApplication"
            id="io.mapzone.atlas.entrypoint"
            path="/atlas">
      </entrypoint>
   </extension>
   <extension
         point="org.polymap.rhei.batik.design">
      <design
            class="io.mapzone.atlas.ui.AtlasAppDesign"
            id="io.mapzone.atlas.design"
            servletNameMatcher="/atlas">
      </design>
   </extension>
   <extension
         point="org.polymap.rhei.batik.panels">
      <panel
            class="io.mapzone.atlas.ui.SearchPanel"
            id="io.mapzone.atlas.AtlasSearchPanel"
            stackPriority="100">
      </panel>
      <panel
            class="io.mapzone.atlas.ui.AtlasMapPanel"
            id="io.mapzone.atlas.AtlasMapPanel">
      </panel>
      <panel
            class="io.mapzone.atlas.ui.AtlasLayersPanel"
            id="io.mapzone.atlas.AtlasLayersPanel"
            stackPriority="10">
      </panel>
   </extension>
   <extension
         point="org.polymap.rhei.batik.panelFilters">
      <panelFilter
            class="io.mapzone.atlas.ui.AtlasPanelFilter"
            servletNameMatcher="/atlas">
      </panelFilter>
   </extension>

</plugin>
